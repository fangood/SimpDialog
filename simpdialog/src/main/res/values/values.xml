    <color name="transparent_no">#02FFFFFF</color>
    <declare-styleable name="PasswordInputView">
        <attr format="integer" name="passwordCount"/>
        <attr format="color" name="stokeColor"/>
        <attr format="color" name="symbolColor"/>
        <attr format="dimension" name="radius"/>
        <attr format="dimension" name="symbolRadius"/>
        <attr format="dimension" name="inputBoxStroke"/>
    </declare-styleable>
    <declare-styleable name="PasswordProgressbar">
        <attr format="reference|color" name="barColor"/>
        <attr format="dimension" name="barStockWidth"/>
        <attr format="dimension" name="barRectWidthHeight"/>
        <attr format="reference|dimension" name="textSize"/>
        <attr format="reference|color" name="textColor"/>
        <attr format="dimension" name="textPaddingLeft"/>
        <attr format="dimension" name="textPaddingRight"/>
        <attr format="dimension" name="textPaddingTop"/>
        <attr format="dimension" name="textPaddingBottom"/>
        <attr format="dimension" name="arcPaddingLeft"/>
        <attr format="dimension" name="arcPaddingRight"/>
        <attr format="dimension" name="arcPaddingTop"/>
        <attr format="dimension" name="arcPaddingBottom"/>
    </declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="divider_line_1px">1px</dimen>
    <dimen name="font_large">17sp</dimen>
    <dimen name="font_large_plus">22sp</dimen>
    <dimen name="font_large_plus_plus">30sp</dimen>
    <dimen name="font_medium">16sp</dimen>
    <dimen name="font_small">14sp</dimen>
    <dimen name="font_small_plus">12sp</dimen>
    <dimen name="margin_all_left_right">16dp</dimen>
    <dimen name="margin_all_top_bottom">16dp</dimen>
    <dimen name="padding_all_left_right">16dp</dimen>
    <dimen name="padding_all_top_bottom">16dp</dimen>
    <dimen name="toolbar_padding_top">0dp</dimen>
    <string name="app_name">DialogAndPop</string>
    <string name="base_ac_cancel">取消</string>
    <string name="base_ac_confirm">确定</string>
    <string name="bottom_pop_middle">相册</string>
    <string name="bottom_pop_top">相机</string>
    <string name="password_input_cancel">取消</string>
    <string name="password_input_forget">忘记密码?</string>
    <string name="password_input_title">请输入支付密码</string>
    <style name="base_dg">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="base_dg_btn_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/padding_all_top_bottom</item>
        <item name="android:textColor">@color/selector_text_black_to_primary</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>
    <style name="base_dg_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:lineSpacingExtra">5dip</item>
        <item name="android:textColor">@color/font_black</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>
    <style name="base_dg_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingExtra">5dip</item>
        <item name="android:textColor">@color/font_black</item>
        <item name="android:textSize">@dimen/font_large</item>
    </style>
    <style name="div_horizontal_1px">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_line_1px</item>
        <item name="android:background">@color/divider_line_color</item>
    </style>
    <style name="div_horizontal_1px_margin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_line_1px</item>
        <item name="android:background">@color/divider_line_color</item>
        <item name="android:layout_marginLeft">@dimen/margin_all_left_right</item>
        <item name="android:layout_marginRight">@dimen/margin_all_left_right</item>
    </style>
    <style name="div_vertical_1px">
        <item name="android:layout_width">@dimen/divider_line_1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/divider_line_color</item>
    </style>
    <style name="slid_bottom_in_out">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_from_bottom</item>
    </style>
</resources>